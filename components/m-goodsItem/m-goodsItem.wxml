<view class="goodsItem">
  <navigator url="/pages/detial/detial?goods_id={{item.goods_id}}">
    <view class="goods-image">
      <image class="goods-imageUr" src="{{item.goods_big_logo?item.goods_big_logo:'https://ae01.alicdn.com/kf/U1891978efa3348e18b549a975d97702bN.jpg'}}" mode="widthFix"/>
    </view>
    <view class="desc-info">
      <view class="tittle">{{item.goods_name}}</view>
      <view class="price">
        <text>￥{{item.goods_price}}</text>
        <text class="orgprice">￥{{item.goods_price}}</text>
      </view>
    </view>
  </navigator>
</view>
